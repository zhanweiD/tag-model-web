.category-tree
  .category-manager-action
    background-color #f0f2f4
  .dt-tree-container
    background-color #fff

// 只有对象和标签节点需要图表
  .dt-tree-line
    .dt-tree-icon
      display flex
    & .dt-tree-icon
      display none
    & .node-tag .dt-tree-icon
      display flex


.select-tag-modal
  min-height 300px
  max-height 500px
  overflow-y auto

  .select-tree
    min-width 150px
    max-width 200px
    margin-left 24px
    overflow-x auto