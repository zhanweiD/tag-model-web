.processe-config
  .ant-form
    .ant-form-item-label
      text-align right !important
      
  .processe-content
    height calc(100vh - 236px)

  .processe-config-step
    padding 30px 80px 0
    margin-bottom 50px

  .bottom-button
    position absolute
    left 0
    bottom 0
    width 100%
    border-top 1px solid #e9e9e9
    padding 10px 16px
    background #fff
    text-align right

// 任务配置
  .form-title
    font-size 14px
    margin-bottom 16px
    position relative
    padding-left 12px

  .form-title:before 
    position absolute
    content ""
    height 14px
    width 3px
    left 0
    top 3px
    background #0078FF 

  .zone-params
    display flex
    flex 1
    justify-content center
    align-items center
    margin-left 24px

  .task-config
    .form-title
      margin-left 230px
      
// 预览保存
  .preview-box
    padding 24px
    background rgba(0,0,0,0.02)

    .form-title
      margin-left 24px

  