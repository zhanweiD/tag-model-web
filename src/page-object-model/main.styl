.page-object-modal
  
  .object-modal-content
    display flex
    min-height: calc(100vh - 48px - 56px - 46px);

  .object-detail
    flex 1
    // width 100%
    padding 16px
    background #F5F6F9

  // 覆盖dtTree样式
  .tree-border
    .dt-tree-box
      border 1px solid #D9D9D9 
      border-radius 4px

  
  .object-tab-content
    background #ffffff
    padding 16px 0px
    min-height calc(100vh - 500px)

  // 对象视图
  .object-view
    width 100%
    height 464px
    color rgba(0, 0, 0, .65)

    .node polygon
      stroke  #AECD73
      fill #E6F0D5 
      stroke-width 1px
    

    .nodes .normal rect
      stroke  #54BAFF
      fill #CBEAFF 
      stroke-width 1px
    
    .nodes .subject rect
      stroke  #FB9B61
      fill #FFE1CF 
      stroke-width 1px

    .edgePath path
      stroke rgba(11,67,156,0.10)
      stroke-width 1px
    
.object-tree
  background #ffffff

.object-tree-action
  display flex
  align-items center
  padding-left 8px
  background-color #f0f2f4


.object-drawer
  // position relative
  .bottom-button
    position absolute
    left 0
    bottom 0
    width 100%
    border-top 1px solid #e9e9e9
    padding 10px 16px
    background #fff
    text-align right