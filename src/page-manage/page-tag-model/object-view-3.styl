.object-view-3
  position relative
  // width 100%
  // height 100%
  width 10000px
  height 10000px
  cursor grab

  .main-node
    position absolute
    width 150px
    padding 12px
    // border 1px solid #F78F73
    background-color #ecf8d8
  
  .main-title
    background-color #e4e4ed
    height 24px
    line-height 24px
    padding-left 8px
  
  .main-key
    background-color rgba(255,255,255,.85)
    height 24px
    line-height 24px
    margin-top 8px
    // padding-left 8px
  
  .main-key span
    margin-left 8px
  
  .main-node-open
    position absolute
    right -10px
    top 28px
    width 18px
    height 18px
    cursor pointer

  .other-node
    position absolute
    width 150px
    padding 12px
    border 1px solid #F78F73
    background-color #FEE6DF

  .other-key
    background-color rgba(255,255,255,.85)
    height 24px
    line-height 24px
    margin-top 8px
    // padding-left 8px

  .other-key span
    margin-left 8px
  
  .other-node-open
    position absolute
    right -10px
    top 15px
    width 18px
    height 18px
    cursor pointer
  
  .diamond 
    position relative
    height 50px
    width 126px
    line-height 50px
    text-align center
    z-index 10

  .diamond svg 
    position absolute
    top 0px
    left 0px
    height 100%
    width 100%
    z-index -1
  
  .diamond span 
    color #fff
    width 60px
    display inline-block
  
.object-view-drawer
  .ant-drawer-body
    margin-bottom 45px