.business-model-2
  position relative
  // width 100%
  // height 100%
  width 10000px
  height 10000px
  cursor grab

  .main-node
    position absolute
    width 120px
    border 1px solid #ecb1aa
    padding 12px 8px
    background-color #ffe5de
  
  .main-title
    background-color #e4e4ed
    height 24px
    line-height 24px
    padding-left 8px
  
  .main-key
    background-color #fff
    height 24px
    line-height 24px
    margin-top 8px
    // padding-left 8px
  
  .main-key span
    margin-left 8px
  
  .main-node-open
    position absolute
    right -10px
    top 15px
    width 18px
    height 18px
    cursor pointer

  .other-node
    position absolute
    width 120px
    padding 12px
    background-color #e9f9d7
  
  .other-key
    background-color #fff
    height 24px
    line-height 24px
    margin-top 8px
    // padding-left 8px

  .other-key span
    margin-left 8px
  
  .other-node-open
    position absolute
    right -10px
    top 28px
    width 18px
    height 18px
    cursor pointer
  
  .diamond 
    position relative
    height 50px
    width 100px
    line-height 50px
    text-align center
    z-index 10

  .diamond svg 
    position absolute
    top 0px
    left 0px
    height 100%
    width 100%
    z-index -1
  
  .diamond span 
    color #fff
  
.business-drawer
  .ant-drawer-body
    margin-bottom 45px