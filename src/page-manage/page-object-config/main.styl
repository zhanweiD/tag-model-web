.page-object
  .object-content
    display flex
    height: calc(100vh - 48px - 56px - 46px);

.object-tree
  // height 100%
  background #ffffff

.object-tree-action
  display flex
  align-items center
  padding-left 8px
  background-color #f0f2f4
  
.object-detail
  flex 1
  height 100%
  padding 16px
  background #F5F6F9
  position relative
  overflow-y auto

  .object-tab-content
    background #ffffff
    padding 16px 0px
    min-height calc(100vh - 500px)
    
.object-detail::-webkit-scrollbar 
  display:none

 // 对象视图
.object-view
  width 100%
  height 464px
  color rgba(0, 0, 0, .65)

  .node polygon
    stroke  #AECD73
    fill #E6F0D5 
    stroke-width 1px
  

  .nodes .normal rect
    stroke  #54BAFF
    fill #CBEAFF 
    stroke-width 1px
  
  .nodes .subject rect
    stroke  #FB9B61
    fill #FFE1CF 
    stroke-width 1px

  .edgePath path
    stroke rgba(11,67,156,0.10)
    stroke-width 1px
  
// 业务视图
.business-model
  width 100%
  height 464px
  color rgba(0, 0, 0, .65)
  overflow hidden